{
  "name": "@groceries/event-sourcing",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "oss/event-sourcing/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "command": "biome lint oss/event-sourcing/src --write"
    },
    "format": {
      "command": "biome format oss/event-sourcing/src --write"
    },
    "check": {
      "command": "biome check oss/event-sourcing/src"
    },
    "ci": {
      "command": "biome ci oss/event-sourcing/src"
    },
    "test": {
      "command": "eval \"$(cat .env)\" && cd oss/event-sourcing && node --import tsx --test 'src/**/*.spec.ts'"
    },
    "build": {
      "command": "tsc -p oss/event-sourcing/tsconfig.json",
      "dependsOn": [],
      "outputs": ["{workspaceRoot}/dist/oss/event-sourcing"]
    }
  }
}
